import java.util.ArrayList;
public class SequenceSum {
	public static int[] sumOfN(int n) {
    
        int numberForMinus = 1;
        int sumForMinus = 0;

    ArrayList<Integer> newList = new ArrayList<>();
    
    if(n>0){
      int sum = 0; 
      for(int i = 1; i<=n; i++){
        sum+= i;
        newList.add(sum);
      }
    } else{
      for(int i = n; i<0; i++){
        sumForMinus = sumForMinus -numberForMinus;
        numberForMinus++;


        newList.add(sumForMinus);
      }
    }
      
int[] answer = new int[newList.size()+1];
    answer[0] = 0; 
    int count = 1; 
    for(Integer one: newList){
      answer[count] = one;
      count++;
    }
    
  for(int i = 0; i<answer.length; i++){
  }
        return answer; //Generated by die roll; guaranteed to be random
  }
}
